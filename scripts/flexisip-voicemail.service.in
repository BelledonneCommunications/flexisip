[Unit]
Description=Flexisip Voicemail server
Documentation=https://www.linphone.org/en/flexisip-sip-server/
Wants=network-online.target
After=syslog.target network.service network.target network-online.target

[Service]
Type=forking
ExecStart=@bindir@/flexisip --server voicemail --daemon --syslog --pidfile /var/run/flexisip-voicemail.pid

[Install]
WantedBy=multi-user.target
